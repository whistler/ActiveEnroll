<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="mylogin.StudentDashboardController">
  <children>
    <HBox prefHeight="78.0" prefWidth="600.0">
      <children>
        <Label prefHeight="64.0" prefWidth="225.0" text="Student Dashboard">
          <effect>
            <Lighting>
              <bumpInput>
                <Shadow />
              </bumpInput>
              <light>
                <javafx.scene.effect.Light.Distant color="#3385ff" />
              </light>
            </Lighting>
          </effect>
          <font>
            <Font name="Kristen ITC" size="20.0" />
          </font>
        </Label>
        <VBox prefHeight="78.0" prefWidth="375.0">
          <children>
            <Label fx:id="loggedUser" contentDisplay="RIGHT" prefHeight="23.0" prefWidth="375.0" textAlignment="RIGHT" textFill="#009962" />
            <HBox alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="375.0">
              <children>
                <Button alignment="CENTER_RIGHT" mnemonicParsing="false" text="Help">
                  <HBox.margin>
                    <Insets right="60.0" />
                  </HBox.margin>
                </Button>
                <Button fx:id="logout" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Logout" textFill="#cc0000">
                  <effect>
                    <DropShadow />
                  </effect>
                  <HBox.margin>
                    <Insets right="30.0" />
                  </HBox.margin>
                </Button>
              </children>
            </HBox>
          </children>
        </VBox>
      </children>
    </HBox>
  </children>
  <stylesheets>
    <URL value="@studentdashboard.css" />
  </stylesheets>
</AnchorPane>
